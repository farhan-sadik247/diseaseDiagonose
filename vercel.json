{
  "buildCommand": "cd disease-diagnosis-frontend && npm install && npm run build",
  "outputDirectory": "disease-diagnosis-frontend/dist",
  "functions": {
    "disease_diagnosis_backend/vercel_wsgi.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/disease_diagnosis_backend/vercel_wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "disease_diagnosis_backend.settings_vercel",
    "PYTHONPATH": "disease_diagnosis_backend"
  }
}
